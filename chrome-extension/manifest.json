{
  "manifest_version": 3,
    "name": "lifeAI.guru - pulpit AI",
    "version": "1.0",
    "description": "Minimalistyczna strona nowej karty z szybkim dostępem do narzędzi AI i wyszukiwaniem",
    "permissions": ["storage"],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self';"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
